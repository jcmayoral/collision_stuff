  <build_depend>geometry_msgs</build_depend>
  <build_depend>mcr_recovery_behaviors</build_depend>

  <run_depend>dynamic_reconfigure</run_depend>
  <run_depend>costmap_2d</run_depend>
  <run_depend>fault_core</run_depend>
