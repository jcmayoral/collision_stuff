  <description>The dynamic_obstacles_recovery package, a recovery strategy for dynamic obstacles collision...</description>

  <maintainer email="jose@todo.todo"> Jose C Mayoral Banos</maintainer>
  <license>GPLv3</license>


  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>cmake_modules</build_depend>
  <build_depend>dynamic_reconfigure</build_depend>
  <build_depend>fault_core</build_depend>
  <build_depend>roscpp</build_depend>
